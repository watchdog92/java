package com.krakedev;

public class Fecha {
	private int anio;
	private int mes;
	private int dia;
	
	public int getAnio() {
		return anio;
	}
	public int getMes() {
		return mes;
	}
	public int getDia() {
		return dia;
	}
	
	public void setAnio(int anio) {
		this.anio=anio;
		
	}
	public void setMes(int mes) {
		this.mes=mes;
		
	}
	public void setDia(int dia) {
		this.dia=dia;
		
	}
	
}
